{
  "Development": {
    "SslSourcePath": "./https/",
    "PreconfiguredDomains": [
      "frodo.digital",
      "samwise.digital",
      "merry.youfoundation.id",
      "pippin.youfoundation.id"
    ]
  },
  "Feed": {
    "InstantDistribution": false,
    "DistributionBatchSize": 100
  },
  "Transit": {
    "OutboxBatchSize": 100
  },
  "Registry": {
    "ProvisioningDomain": "provisioning-dev.youfoundation.id",
    "PowerDnsHostAddress": "dns.id.pub",
    "PowerDnsApiKey": "replace_with_top_secret_powerdns_api_key",
    "ManagedDomains": [
      "id.pub",
      "dev.dominion.id"
    ],
    "FrontendDnsRecords": [{
      "Type": "A",
      "Name": "",
      "Value": "131.164.170.62",
      "Description": "IP address of frontend host"
    },{
      "Type": "CNAME",
      "Name": "www",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of frontend host"
    }],
    "BackendDnsRecords": [{
      "Type": "CNAME",
      "Name": "odin",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of backend host"
    }],
    "StorageDnsRecords": [{
      "Type": "CNAME",
      "Name": "file",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of backend file host"
    }]
  },
  "Host": {
    "CacheSlidingExpirationSeconds": 15,
    "TenantDataRootPath": "/tmp/dotyou/tenants/data",
    "SystemDataRootPath": "/tmp/dotyou/system",
    "IPAddressListenList": [
      {
        "Ip": "*",
        "HttpsPort": 443,
        "HttpPort": 80
      }
    ]
  },
  "Logging": {
    "LogFilePath": "/tmp/dotyou/logs",
    "Level": "Verbose"
  },
  "Quartz": {
    "EnableQuartzBackgroundService": true,
    "CronBatchSize": 100,
    "BackgroundJobStartDelaySeconds": 10,
    "CronProcessingInterval": 2,
    "EnsureCertificateProcessorIntervalSeconds": 40,
    "ProcessPendingCertificateOrderIntervalInSeconds": 5
  },
  "CertificateRenewal": {
    "NumberOfCertificateValidationTries": 3,
    "UseCertificateAuthorityProductionServers": false,
    "CertificateAuthorityAssociatedEmail": "admin@nowhere.co",
    "CsrCountryName": "US",
    "CsrState": "CA",
    "CsrLocality": "Berkeley",
    "CsrOrganization": "YF",
    "CsrOrganizationUnit": "Dev"
  }
}
