{
  "Development": {
    "SslSourcePath": "./https/",
    "PreconfiguredDomains": [
      "frodobaggins.me",
      "samwisegamgee.me"
    ]
  },
  "Feed": {
    "InstantDistribution": false,
    "DistributionBatchSize": 100
  },
  "Transit": {
    "OutboxBatchSize": 100
  },
  "Registry": {
    "ProvisioningDomain": "setup.youfoundation.id",
    "PowerDnsHostAddress": "dns.id.pub",
    "PowerDnsApiKey": "replace_with_top_secret_powerdns_api_key",
    "ManagedDomainApexes": [{
      "Apex": "id.pub",
      "PrefixLabels": ["First", "Surname"]
    },
      {
        "Apex": "dev.dominion.id",
        "PrefixLabels": ["Your Identity Here"]
      }],
    "FrontendDnsRecords": [{
      "Type": "A",
      "Name": "",
      "Value": "131.164.170.62",
      "Description": "IP address of frontend host"
    },{
      "Type": "CNAME",
      "Name": "www",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of frontend host"
    }],
    "BackendDnsRecords": [{
      "Type": "CNAME",
      "Name": "api",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of backend host"
    },{
      "Type": "CNAME",
      "Name": "capi",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of backend host with client certificates"
    }],
    "StorageDnsRecords": [{
      "Type": "CNAME",
      "Name": "file",
      "Value": "{{domain-placeholder}}",
      "Description": "Alias name of backend file host"
    }],
    "DnsResolvers": [
      "1.1.1.1",
      "8.8.8.8",
      "9.9.9.9",
      "208.67.222.222"
    ]
  },
  "Host": {
    "CacheSlidingExpirationSeconds": 60,
    "TenantDataRootPath": "/tmp/dotyou/tenants/data",
    "SystemDataRootPath": "/tmp/dotyou/system",
    "IPAddressListenList": [
      {
        "Ip": "*",
        "HttpsPort": 443,
        "HttpPort": 80
      }
    ]
  },
  "Logging": {
    "LogFilePath": "/tmp/dotyou/logs",
    "Level": "Verbose"
  },
  "Quartz": {
    "EnableQuartzBackgroundService": true,
    "CronBatchSize": 100,
    "BackgroundJobStartDelaySeconds": 10,
    "CronProcessingInterval": 2,
    "EnsureCertificateProcessorIntervalSeconds": 500,
    "ProcessPendingCertificateOrderIntervalInSeconds": 5
  },
  "CertificateRenewal": {
    "NumberOfCertificateValidationTries": 3,
    "UseCertificateAuthorityProductionServers": true,
    "CertificateAuthorityAssociatedEmail": "cert@vidence.co"
  }
}
