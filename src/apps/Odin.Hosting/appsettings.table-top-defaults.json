{
  "Registry": {
    "InvitationCodes": [
      "secret!"
    ],
    "ProvisioningDomain": "your-provisioning-domain-here.com",
    "ProvisioningEnabled": true,
    "DnsRecordValues": {
      "ApexARecords": [
        "123.123.123.123"
      ],
      "ApexAliasRecord": "your-apex-alias-record.com"
    }
  },
  "Host": {
    "CacheSlidingExpirationSeconds": 15,
    "TenantDataRootPath": "$HOME/tmp/dotyou/tenants",
    "SystemDataRootPath": "$HOME/tmp/dotyou/system",
    "IPAddressListenList": [
      {
        "Ip": "*",
        "HttpsPort": 443,
        "HttpPort": 80
      }
    ]
  },
  "Logging": {
    "LogFilePath": "$HOME/tmp/dotyou/logs"
  },
  "Job": {
    "EnsureCertificateProcessorIntervalSeconds": 3600,
    "InboxOutboxReconciliationIntervalSeconds": 43200,
    "JobCleanUpIntervalSeconds": 14400
  },
  "CertificateRenewal": {
    "UseCertificateAuthorityProductionServers": true,
    "CertificateAuthorityAssociatedEmail": "noreply@homebase.id"
  }
}
