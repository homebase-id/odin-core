{
  "Development": {
    "RecoveryKeyWaitingPeriodSeconds": 60,
    "SslSourcePath": "./https/",
    "PreconfiguredDomains": [
      "frodo.dotyou.cloud",
      "sam.dotyou.cloud",
      "merry.dotyou.cloud",
      "pippin.dotyou.cloud",
      "tom.dotyou.cloud",
      "collab.dotyou.cloud"
    ]
  },
  "Registry": {
    "InvitationCodes": [
      "anabolicfrolic"
    ],
    "ProvisioningDomain": "provisioning.dotyou.cloud",
    "ProvisioningEnabled": true,
    "PowerDnsHostAddress": "your-power-dns-host-address-here.id",
    "PowerDnsApiKey": "replace_with_top_secret_powerdns_api_key",
    "ManagedDomainApexes": [
      {
        "Apex": "demo.rocks",
        "PrefixLabels": [
          "First",
          "Surname"
        ]
      }
    ],
    "DnsRecordValues": {
      "ApexARecords": [
        "123.123.123.123"
      ],
      "ApexAliasRecord": "your-apex-alias-record.id"
    },
    "DnsResolvers": [
      "1.1.1.1",
      "8.8.8.8",
      "9.9.9.9",
      "208.67.222.222"
    ]
  },
  "Host": {
    "ReportContentUrl": "your-report-content-url-here",
    "CacheSlidingExpirationSeconds": 15,
    "TenantDataRootPath": "$HOME/tmp/dotyou/tenants",
    "ShutdownTimeoutSeconds": 120,
    "SystemDataRootPath": "$HOME/tmp/dotyou/system",
    "Http1Only": false,
    "IPAddressListenList": [
      {
        "Ip": "*",
        "HttpsPort": 443,
        "HttpPort": 80
      }
    ]
  },
  "Logging": {
    "LogFilePath": "$HOME/tmp/dotyou/logs"
  },
  "Job": {
    "EnsureCertificateProcessorIntervalSeconds": 3600,
    "InboxOutboxReconciliationIntervalSeconds": 43200,
    "JobCleanUpIntervalSeconds": 14400
  },
  "CertificateRenewal": {
    "UseCertificateAuthorityProductionServers": false,
    "CertificateAuthorityAssociatedEmail": "noreply@homebase.id"
  },
  "Admin": {
    "ApiEnabled": true,
    "ApiKey": "your-secret-api-key-here",
    "ApiKeyHttpHeaderName": "Odin-Admin-Api-Key",
    "ApiPort": 4444,
    "Domain": "admin.dotyou.cloud",
    "ExportTargetPath": "/tmp/odin-export"
  },
  "Database": {
    "Type": "sqlite",
    "ConnectionString": "(not-used-in-sqlite)"
  },
  "Redis": {
    "Enabled": false,
    "Configuration": "your-redis-configuration-here"
  },
  "Cache": {
    "Level2CacheType": "none"
  }

}
