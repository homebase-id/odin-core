{
  "Development": {
    "RecoveryKeyWaitingPeriodSeconds": 60,
    "SslSourcePath": "./https/",
    "PreconfiguredDomains": [
      "frodo.dotyou.cloud",
      "sam.dotyou.cloud",
      "merry.dotyou.cloud",
      "pippin.dotyou.cloud",
      "tom.dotyou.cloud"
    ]
  },
  "Registry": {
    "InvitationCodes": [
      "anabolicfrolic"
    ],
    "ProvisioningDomain": "provisioning.dotyou.cloud",
    "ProvisioningEmailLogoImage": "https://homebase.id/logo-email.png",
    "ProvisioningEmailLogoHref": "https://homebase.id/",
    "ProvisioningEnabled": true,
    "PowerDnsHostAddress": "dns.id.pub",
    "PowerDnsApiKey": "replace_with_top_secret_powerdns_api_key",
    "ManagedDomainApexes": [
      {
        "Apex": "demo.rocks",
        "PrefixLabels": [
          "First",
          "Surname"
        ]
      },
      {
        "Apex": "sebbarg.dk",
        "PrefixLabels": [
          "Name"
        ]
      }
    ],
    "DnsRecordValues": {
      "ApexARecords": [
        "131.164.170.62"
      ],
      "ApexAliasRecord": "identity-host.sebbarg.net"
    },
    "DnsResolvers": [
      "1.1.1.1",
      "8.8.8.8",
      "9.9.9.9",
      "208.67.222.222"
    ]
  },
  "Host": {
    "ReportContentUrl": "your-report-content-url-here",
    "CacheSlidingExpirationSeconds": 15,
    "TenantDataRootPath": "$HOME/homebase/dev/tenants",
    "ShutdownTimeoutSeconds": 30,
    "SystemDataRootPath": "$HOME/homebase/dev/system",
    "Http1Only": false,
    "IPAddressListenList": [
      {
        "Ip": "*",
        "HttpsPort": 443,
        "HttpPort": 80
      }
    ]
  },
  "Logging": {
    "LogFilePath": "$HOME/homebase/dev/logs"
  },
  "Job": {
    "EnsureCertificateProcessorIntervalSeconds": 3600,
    "InboxOutboxReconciliationIntervalSeconds": 43200,
    "JobCleanUpIntervalSeconds": 14400
  },
  "CertificateRenewal": {
    "UseCertificateAuthorityProductionServers": false,
    "CertificateAuthorityAssociatedEmail": "noreply@homebase.id"
  },
  "Mailgun": {
    "ApiKey": "your-mailgun-api-key-here",
    "DefaultFromEmail": "your-default-email-from-address-here@example.com",
    "DefaultFromName": "",
    "EmailDomain": "your-mailgun-email-domain-here",
    "Enabled": false
  },
  "Admin": {
    "ApiEnabled": true,
    "ApiKey": "your-secret-api-key-here",
    "ApiKeyHttpHeaderName": "Odin-Admin-Api-Key",
    "ApiPort": 4444,
    "Domain": "admin.dotyou.cloud",
    "ExportTargetPath": "/tmp/odin-export"
  },
  "PushNotification": {
    "BaseUrl": "https://push.homebase.id"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Error"
      }
    }
  }
}
